{
	"semi": true,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 4,
	"printWidth": 160,
	"proseWrap": "never",
	"trailingComma": "es5",
	"bracketSameLine": false,
	"singleAttributePerLine": true,
	"importOrder": [
		"^@core/(.*)$",
		"^@server/(.*)$",
		"^@ui/(.*)$",
		"^[./@]",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
